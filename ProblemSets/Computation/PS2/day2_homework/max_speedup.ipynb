   "source": [
    "# Maximum Speed-up"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def max_speed(serial, CPU):\n",
    "    return 1/(serial+(1-serial)/100)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The maximum speed-up that can be optained is: 71.6332%\n"
     ]
    }
   ],
   "source": [
    "serial= 0.004\n",
    "CPU = 100\n",
    "speedup=max_speed(serial, CPU)\n",
    "round_speedup=round(speedup,4)\n",
    "print(\"The maximum speed-up that can be optained is:\", str(round_speedup)+\"%\")"
   ]
  }
